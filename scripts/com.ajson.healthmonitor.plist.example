<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.ajson.healthmonitor</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>{{AJSON_ROOT}}/scripts/monitor_health.sh</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>StartInterval</key>
    <integer>60</integer>
    
    <key>StandardOutPath</key>
    <string>{{AJSON_ROOT}}/logs/monitor_stdout.log</string>
    
    <key>StandardErrorPath</key>
    <string>{{AJSON_ROOT}}/logs/monitor_stderr.log</string>
    
    <key>WorkingDirectory</key>
    <string>{{AJSON_ROOT}}</string>
</dict>
</plist>
